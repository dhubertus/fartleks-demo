{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "semi": [2],
    "class-methods-use-this": [0],
    "no-trailing-spaces": [0],
    "no-plusplus": [0],
    "import/prefer-default-export": [0],
    "arrow-body-style": [0]
  }
}
